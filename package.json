  "main": "server/dist/app.js",
  "scripts": {
    "ng": "ng",
    "start": "node .",
    "start:dev": "cd server && nodemon app.ts",
    "prebuild": "npm run lint",
    "build": "npm run build-server && npm run build-client:prod",
    "build-client": "ng build --configuration production",
    "build-client:prod": "ng build --configuration production",
    "build-client:dev": "ng build",
    "build-client:watch": "ng build --watch",
    "build-server": "tsc -p ./server",
    "test": "ng test",
    "lint": "npm run lint-server && npm run lint-client",
    "lint-client": "ng lint",
    "lint-server": "eslint -c ./server/.eslintrc.json --ext .ts ./server",
    "e2e": "ng e2e",
    "postinstall": "npm run build",
    "migration": "npm run build-server && node ./server/dist/migrations/%npm_config_filename%.js"
  },
  "private": true,
